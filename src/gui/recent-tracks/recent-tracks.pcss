.container {
	width: 400px;
	margin-left: 10%;
}

.header {
	display: flex;
	align-items: center;
	gap: 30px;
	margin-bottom: 20px;
	border-bottom: 1px solid $color-black-1;
}

.list {
	list-style: none;
	display: flex;
	flex-direction: column;
	gap: 10px;
	height: 80vh;
	overflow: scroll;
}

.button {
	height: 100%;
}
